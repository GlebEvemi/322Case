/* dbdiagram.io */
Table users {
  id int pk
  username varchar
  password varchar
  balance bigint
}

Table cases {
  id int pk
  name varchar
  img_url varchar
  price bigint
}

Table items {
  id int pk
  name varchar
  img_url varchar
  price bigint
}

Table user_item {
  user_id int
  item_id int
}


Ref: users.id <> user_item.user_id
Ref: items.id <> user_item.item_id
